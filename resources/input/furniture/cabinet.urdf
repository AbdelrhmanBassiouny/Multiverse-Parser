<?xml version="1.0"?>
<robot name="apartment">
    <material name="sink">
        <color rgba="0.6 0.6 0.6 1" />
    </material>
    <material name="cabinet_surface">
        <color rgba="0.3 0.3 0.3 1" />
    </material>
    <material name="cooktop_surface">
        <color rgba="0.2 0.2 0.2 1" />
    </material>
    <material name="cabinet_handle">
        <color rgba="0.8 0.8 0.8 1" />
    </material>
    <material name="coloksu">
        <color rgba="0.8 0.0 0.0 1" />
    </material>
    <material name="countertop">
        <color rgba="0.85 0.51 0.16 1" />
    </material>
    <material name="island_sides">
        <color rgba="0.3 0.3 0.3 1" />
    </material>
    <link name="apartment_root">
        <origin rpy="0 0 0" xyz="0 0 0" />
    </link>
    <joint name="furnitures_base" type="fixed">
        <origin xyz="8.85 1.75 0.0" />
        <parent link="apartment_root" />
        <child link="furnitures_root" />
    </joint>
    <link name="furnitures_root" />
    <joint name="kitchen_base" type="fixed">
        <origin xyz="-0.03 0.75 0.005" />
        <parent link="apartment_root" />
        <child link="kitchen_root" />
    </joint>
    <link name="kitchen_root" />
    <!-- Frames, reference frames -->
    <joint name="frame_A_joint" type="fixed">
        <parent link="kitchen_root" />
        <origin xyz="0.49 0.696 0.080" />
        <child link="side_A" />
    </joint>
    <!-- Bond, James Bond -->
    <joint name="frame_B_joint" type="fixed">
        <parent link="kitchen_root" />
        <origin rpy="0 0 3.14" xyz="2.91 3.7 0.11" />
        <child link="side_B" />
    </joint>
    <link name="side_A" />
    <link name="side_B" />
    <!-- - - - - - - - - - - - - Cabinets side A - - - - - - - - - -->
    <!-- Cabinet1 -->
    <joint name="cabinet1_joint" type="fixed">
        <origin xyz="0.01 0.01 1.02" />
        <parent link="side_A" />
        <child link="cabinet1" />
    </joint>
    <link name="cabinet1">
        <collision>
            <origin xyz="0.0 0.0 0.0" />
            <geometry>
                <mesh
                    filename="file://./obj/IAICabinet1W60.obj" />
            </geometry>
        </collision>
    </link>
    <joint name="cabinet1_door_top_left_joint" type="revolute">
        <origin rpy="0 0 3.14" xyz="0.103 -0.3 0.32" />
        <parent link="cabinet1" />
        <child link="cabinet1_door_top_left" />
        <axis xyz="0 0 -1" />
        <limit effort="1000.0" lower="0.0" upper="1.5708" velocity="1.5708" />
    </joint>
    <link name="cabinet1_door_top_left">
        <collision>
            <origin rpy="0 0 3.14" xyz="0.001 -0.305 0.35" />
            <geometry>
                <mesh
                    filename="file://./obj/IAIDoorW60H80.obj" />
            </geometry>
        </collision>
    </link>
    <joint name="handle_cab1_top_door_joint" type="fixed">
        <origin rpy="1.57 0 0" xyz="-0.055 -0.5 0.0" />
        <parent link="cabinet1_door_top_left" />
        <child link="handle_cab1_top_door" />
    </joint>
    <link name="handle_cab1_top_door">
        <collision>
            <geometry>
                <mesh
                    filename="file://./obj/handle_horizontal.obj" />
            </geometry>
        </collision>
    </link>
    <joint name="oven_joint" type="fixed">
        <origin rpy="0 0 0" xyz="-0.175 0.0 -0.025" />
        <!-- <origin xyz="-0.125 0.0 -0.025" rpy="0 0 0"/> -->
        <parent link="cabinet1" />
        <child link="oven" />
    </joint>
    <material name="oven_gray">
        <color rgba="0.5 0.5 0.5 1" />
    </material>
    <link name="oven">
        <collision>
            <geometry>
                <box size="0.58 0.58 0.59" />
            </geometry>
        </collision>
    </link>
    <joint name="cabinet1_drawer_middle_joint" type="prismatic">
        <origin rpy="0 0 3.14" xyz="-0.117 0.0 -0.43" />
        <parent link="cabinet1" />
        <child link="cabinet1_drawer_middle" />
        <axis xyz="-1 0 0" />
        <limit effort="1000.0" lower="0.0" upper="0.45" velocity="0.5" />
    </joint>
    <link name="cabinet1_drawer_middle">
        <collision>
            <origin rpy="0 0 3.14" />
            <geometry>
                <mesh
                    filename="file://./obj/IAIDrawerW60H26.obj" />
            </geometry>
        </collision>
    </link>
    <joint name="handle_cab1_drawer_mid_joint" type="fixed">
        <origin rpy="1.57 0 0" xyz="-0.26 0.0 0.11" />
        <parent link="cabinet1_drawer_middle" />
        <child link="handle_cab1_drawer_mid" />
    </joint>
    <link name="handle_cab1_drawer_mid">
        <collision>
            <geometry>
                <mesh
                    filename="file://./obj/handle_horizontal.obj" />
            </geometry>
        </collision>
    </link>
    <joint name="cabinet1_drawer_bottom_joint" type="prismatic">
        <origin rpy="0 0 3.14" xyz="-0.094 0.0 -0.95" />
        <parent link="cabinet1" />
        <child link="cabinet1_drawer_bottom" />
        <axis xyz="-1 0 0" />
        <limit effort="1000.0" lower="0.0" upper="0.45" velocity="0.5" />
    </joint>
    <link name="cabinet1_drawer_bottom">
        <collision>
            <origin rpy="0 0 -3.14" />
            <geometry>
                <mesh
                    filename="file://./obj/IAIDrawerW60H53.obj" />
            </geometry>
        </collision>
    </link>
    <joint name="handle_cab1_drawer_bottom_joint" type="fixed">
        <origin rpy="1.57 0 0" xyz="-0.237 0.0 0.35" />
        <parent link="cabinet1_drawer_bottom" />
        <child link="handle_cab1_drawer_bottom" />
    </joint>
    <link name="handle_cab1_drawer_bottom">
        <collision>
            <geometry>
                <mesh
                    filename="file://./obj/handle_horizontal.obj" />
            </geometry>
        </collision>
    </link>
    <!-- Collision okay surface for cabinet1-->
    <material name="white">
        <color rgba="1 1 1 1" />
    </material>
    <joint name="cabinet1_coloksu_level4_joint" type="fixed">
        <origin xyz="-0.172 0.005 0.281" />
        <parent link="cabinet1" />
        <child link="cabinet1_coloksu_level4" />
    </joint>
    <link name="cabinet1_coloksu_level4">
        <collision>
            <geometry>
                <box size="0.534 0.568 0.003" />
            </geometry>
        </collision>
    </link>
    <joint name="cabinet1_coloksu_level5_joint" type="fixed">
        <!-- actually this should align with the CAD model but the CAD model is incorrect. -->
        <!-- <origin xyz="-0.172 0.005 0.672"/> -->
        <origin xyz="-0.172 0.005 0.56" />
        <parent link="cabinet1" />
        <child link="cabinet1_coloksu_level5" />
    </joint>
    <link name="cabinet1_coloksu_level5">
        <collision>
            <geometry>
                <box size="0.534 0.568 0.02" />
            </geometry>
        </collision>
    </link>
</robot>